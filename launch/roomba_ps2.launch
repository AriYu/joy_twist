<launch>

<!-- joy_node -->
<node respawn="true" pkg="joy" type="joy_node" name="joy">
  <param name="dev" type="string" value="/dev/input/js0" />
  <param name="deadzone" value="0.12" />
  <param name="autorepeat_rate" value="25" />
</node>

<!-- nishidalab_ypspur_joy_controler -->
<node pkg="nishidalab_ypspur_joy_controler"   type="nishidalab_ypspur_joy_controler_node"  name="nishidalab_ypspur_joy_controler" >
  <rosparam command="load" ns="/nishidalab_ypspur" file="$(find nishidalab_ypspur_driver)/config/nishidalab_ypspur_driver.yaml" />
  <rosparam command="load" ns="/nishidalab_ypspur" file="$(find nishidalab_ypspur_joy_controler)/config/ps2.yaml" />
</node>

</launch>
